{
    "id": "org.gimp.BaseApp",
    "branch": "stable",
    "runtime": "org.gnome.Platform",
    "sdk": "org.gnome.Sdk",
    "runtime-version": "3.30",
    "command": "/usr/bin/bash",
    "build-options" : {
        "cflags": "-O2 -g",
        "cxxflags": "-O2 -g",
        "env": {
            "V": "1"
        }
    },
    "cleanup": [ "/man", "/share/man", "/share/gtk-doc",
                 "/share/vala", "*.la", "*.a"],
    "modules": [
        "org.gimp.GIMP.base-modules.json"
    ]
}
